        lv2:index 2 ;
	lv2:name "BPM" ;
        lv2:symbol "BPM" ;
        lv2:default 120 ;
        lv2:minimum 20 ;
        lv2:maximum 280 ;
        lv2:portProperty  mod:tempoRelatedDynamicScalePoints;
	units:unit units:bpm ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "Div_L" ;
        lv2:symbol "Div_L" ;
        lv2:default 1 ;
        lv2:minimum 1 ;
        lv2:maximum 10 ;
	lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;    
        lv2:scalePoint [rdfs:label "1/4note"; rdf:value 1];
        lv2:scalePoint [rdfs:label "Dotted1/8note"; rdf:value 2];
        lv2:scalePoint [rdfs:label "1/4notetriplets"; rdf:value 3];
        lv2:scalePoint [rdfs:label "1/8note"; rdf:value 4];
        lv2:scalePoint [rdfs:label "1/8notetriplets"; rdf:value 5];
        lv2:scalePoint [rdfs:label "Dotted1/16note"; rdf:value 6];
        lv2:scalePoint [rdfs:label "5tuplet"; rdf:value 7];
        lv2:scalePoint [rdfs:label "7tuplet"; rdf:value 8];
        lv2:scalePoint [rdfs:label "9tuplet"; rdf:value 9];
        lv2:scalePoint [rdfs:label "11tuplet"; rdf:value 10];
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "ENV_ms" ;
        lv2:symbol "ENV_ms" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 500 ;
    ] ,
    [
        a lv2:OutputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "LEFT" ;
        lv2:symbol "LEFT" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "RANDOM" ;
        lv2:symbol "RANDOM" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
	lv2:portProperty lv2:toggled;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "RANGE" ;
        lv2:symbol "RANGE" ;
        lv2:default 5 ;
        lv2:minimum 1 ;
        lv2:maximum 10 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "dry_wet" ;
        lv2:symbol "dry_wet" ;
        lv2:default 0.5 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "reverse" ;
        lv2:symbol "reverse" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
	lv2:portProperty lv2:toggled;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 10 ;
        lv2:name "speed" ;
        lv2:symbol "speed" ;
        lv2:default 1 ;
        lv2:minimum 0.00899999961257 ;
        lv2:maximum 2 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 11 ;
        lv2:name "ztutter" ;
        lv2:symbol "ztutter" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
	lv2:portProperty lv2:integer, lv2:toggled, mod:preferMomentaryOnByDefault ;
    ] ;

